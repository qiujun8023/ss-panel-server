{
  "apps": [
    {
      "name": "ss-panel",
      "script": "src/index.js",
      "env": {
        "NODE_ENV": "production"
      },
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "exec_mode": "cluster",
      "instances": 0,
      "watch": false,
      "merge_logs": true,
      "autorestart": true,
      "error_file": "/tmp/ss-panel.log",
      "out_file": "/tmp/ss-panel.log"
    }
  ]
}
